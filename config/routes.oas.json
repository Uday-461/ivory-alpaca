{
  "openapi": "3.0.0",
  "info": {
    "title": "MCP API Gateway - Minimal Test",
    "version": "1.0.0"
  },
  "paths": {
    "/api/servers": {
      "post": {
        "summary": "Register Server - Test",
        "operationId": "registerServer",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        },
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "default",
            "module": "$import(./modules/routes/servers/register)"
          },
          "policies": {
            "inbound": []
          }
        }
      }
    },
    "/api/health": {
      "get": {
        "summary": "Health Check",
        "operationId": "healthCheck",
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "default",
            "module": "$import(./modules/routes/health)",
            "options": {}
          },
          "policies": {
            "inbound": []
          }
        }
      }
    }
  }
}